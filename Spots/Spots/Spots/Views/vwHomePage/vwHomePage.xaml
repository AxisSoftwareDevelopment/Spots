<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Spots.Views.vwHomePage.vwHomePage"
             xmlns:pages="clr-namespace:Spots.Views.vwHomePage">
  <FlyoutPage.Flyout>
    <pages:vwHomePageFlyout x:Name="FlyoutPage"/>
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:vwHomePageDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>