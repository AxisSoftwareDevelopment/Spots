<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    
    <!-- Default Values -->
    
    <Color x:Key="Primary">#FFA500</Color>
    <Color x:Key="Secondary">White</Color>
    <Color x:Key="Tertiary">#757575</Color>
    <Color x:Key="PrimaryAccent">White</Color>
    <Color x:Key="SecondaryAccent">Black</Color>
    <Color x:Key="ErrorAccent">Red</Color>
    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="Gray100">#E1E1E1</Color>
    <Color x:Key="Gray200">#C8C8C8</Color>
    <Color x:Key="Gray300">#ACACAC</Color>
    <Color x:Key="Gray400">#919191</Color>
    <Color x:Key="Gray500">#6E6E6E</Color>
    <Color x:Key="Gray600">#404040</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#141414</Color>

    <!-- My Color Themes -->

    <Color x:Key="DarkPrimary">#FFA500</Color>
    <Color x:Key="DarkSecondary">#212121</Color>
    <Color x:Key="DarkTertiary">#757575</Color>
    <Color x:Key="DarkPrimaryAccent">White</Color>
    <Color x:Key="DarkSecondaryAccent">White</Color>

    <Color x:Key="LightPrimary">#FFA500</Color>
    <Color x:Key="LightSecondary">#DFD8F7</Color>
    <Color x:Key="LightTertiary">#757575</Color>
    <Color x:Key="LightPrimaryAccent">White</Color>
    <Color x:Key="LightSecondaryAccent">Black</Color>

</ResourceDictionary>